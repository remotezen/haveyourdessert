<h1>Log in</h1>
<span class="sessions-new">

<%form_tag url_for(:sessions,:create), method:'post' do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p class = "center">
  <%= submit_tag "Log in", class: "button radius" %>
  </p>
<% end %>
</span>


